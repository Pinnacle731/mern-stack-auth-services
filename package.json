{
  "name": "auth-service-typescript",
  "version": "1.0.0",
  "description": "auth-service create using typescript nodejs",
  "main": "src/server.ts",
  "scripts": {
    "test:watch": "jest --passWithNoTests --watch --coverage --all --runInBand",
    "test": "jest --no-cache --coverage --all --runInBand",
    "build": "tsc",
    "start": "ts-node dist/src/server.js",
    "dev": "nodemon --legacy-watch src/server.ts",
    "typeorm": "npm run build && npx typeorm-ts-node-esm -d dist/database/datasource.js",
    "migration:show": "npm run typeorm migration:show",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate",
    "migration:run": "typeorm-ts-node-commonjs migration:run",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "author": "parth dangroshiya",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@jest/globals": "^30.0.5",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "express-validator": "^7.2.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.1.4",
    "mock-jwks": "^1.0.10",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "swagger-jsdoc": "^6.2.8",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.1.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "winston": "^3.17.0",
    "yargs": "^18.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md,mjs}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@types/mongoose": "^5.11.97",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "http-errors": "^2.0.0",
    "jwks-rsa": "^3.2.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rsa-pem-to-jwk": "^1.1.3",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0"
  }
}
