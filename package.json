{
  "name": "auth-service-typescript",
  "version": "1.0.0",
  "description": "auth-service create using typescript nodejs",
  "main": "src/server.ts",
  "scripts": {
    "test:watch": "jest --passWithNoTests --watch --coverage --all --runInBand",
    "test": "jest --no-cache --coverage --all --runInBand",
    "build": "tsc",
    "start": "ts-node dist/server.js",
    "dev": "nodemon --legacy-watch src/server.ts",
    "typeorm": "npm run build && npx typeorm-ts-node-esm -d dist/database/datasourcemigration.js",
    "migration:show": "npm run typeorm migration:show",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate",
    "migration:run": "typeorm-ts-node-commonjs migration:run",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "author": "parth dangroshiya",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express-validator": "^7.2.0",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.3.0",
    "mock-jwks": "^1.0.10",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "winston": "^3.10.0",
    "yargs": "^17.7.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md,mjs}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@types/mongoose": "^5.11.96",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "http-errors": "^2.0.0",
    "jwks-rsa": "^3.1.0",
    "mongodb": "^5.9.2",
    "mongoose": "^8.9.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rsa-pem-to-jwk": "^1.1.3",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25",
    "uuid": "^11.0.3"
  }
}
